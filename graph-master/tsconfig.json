    "compilerOptions": {
        "outDir": "./build",
        "sourceMap": true,
        "noImplicitAny": false,
        "removeComments": true,
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
    },
    "exclude": [
        "node_modules",
        "build"
    ]
}